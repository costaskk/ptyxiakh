th > 0) throw new Error('\"endReadable()\" called on non-empty stream');\n\n  if (!state.endEmitted) {\n    state.ended = true;\n    pna.nextTick(endReadableNT, state, stream);\n  }\n}\n\nfunction endReadableNT(state, stream) {\n  // Check that we didn't get one last unshift.\n  if (!state.endEmitted && state.length === 0) {\n    state.endEmitted = true;\n    stream.readable = false;\n    stream.emit('end');\n  }\n}\n\nfunction indexOf(xs, x) {\n  for (var i = 0, l = xs.length; i < l; i++) {\n    if (xs[i] === x) return i;\n  }\n  return -1;\n}","name":"node_modules/readable-stream/lib/_stream_readable.js"},"_buildHash":"fc15030834ff0469d78978f10fda509e","hash":"f300a4e2bcb909b8f0f604e7618f923b","_lastSuccessfulBuildMeta":{"providedExports":true}},"dependencyBlock":{"type":"DependenciesBlock","dependencies":[{"type":"ConstDependency","expression":"","range":[1134,1147],"loc":{"start":{"line":22,"column":0},"end":{"line":22,"column":13}}},{"type":"CommonJsRequireDependency","request":"process-nextick-args","range":[1186,1208],"loc":{"start":{"line":26,"column":10},"end":{"line":26,"column":41}}},{"type":"RequireHeaderDependency","range":[1178,1185],"loc":{"start":{"line":26,"column":10},"end":{"line":26,"column":41}}},{"type":"RequireHeaderDependency","range":[1291,1298],"loc":{"start":{"line":32,"column":14},"end":{"line":32,"column":32}}},{"type":"CommonJsRequireDependency","request":"isarray","range":[1299,1308],"loc":{"start":{"line":32,"column":14},"end":{"line":32,"column":32}}},{"type":"CommonJsRequireDependency","request":"events","range":[1457,1465],"loc":{"start":{"line":42,"column":9},"end":{"line":42,"column":26}}},{"type":"RequireHeaderDependency","range":[1449,1456],"loc":{"start":{"line":42,"column":9},"end":{"line":42,"column":26}}},{"type":"RequireHeaderDependency","range":[1626,1633],"loc":{"start":{"line":50,"column":13},"end":{"line":50,"column":49}}},{"type":"CommonJsRequireDependency","request":"./internal/streams/stream","range":[1634,1661],"lo