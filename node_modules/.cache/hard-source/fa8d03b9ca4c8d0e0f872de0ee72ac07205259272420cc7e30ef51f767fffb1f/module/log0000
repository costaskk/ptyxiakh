\n    this._writableState.ending = false;\n    this._writableState.finished = false;\n    this._writableState.errorEmitted = false;\n  }\n}\n\nfunction emitErrorNT(self, err) {\n  self.emit('error', err);\n}\n\nmodule.exports = {\n  destroy: destroy,\n  undestroy: undestroy\n};","name":"node_modules/readable-stream/lib/internal/streams/destroy.js"},"_buildHash":"4b35d764118f7a6e05aca505afcc3315","hash":"7d52bbfaebb898cf8dea87e65378d256","_lastSuccessfulBuildMeta":{"providedExports":true}},"dependencyBlock":{"type":"DependenciesBlock","dependencies":[{"type":"ConstDependency","expression":"","range":[0,13],"loc":{"start":{"line":1,"column":0},"end":{"line":1,"column":13}}},{"type":"CommonJsRequireDependency","request":"process-nextick-args","range":[52,74],"loc":{"start":{"line":5,"column":10},"end":{"line":5,"column":41}}},{"type":"RequireHeaderDependency","range":[44,51],"loc":{"start":{"line":5,"column":10},"end":{"line":5,"column":41}}}],"variables":[],"blocks":[]},"source":{"renderedHash":"7d52bbfaebb898cf8dea"}}{"type":"NormalModule","constructor":{"data":{"type":"javascript/auto","request":"node_modules/readable-stream/lib/internal/streams/stream-browser.js","userRequest":"node_modules/readable-stream/lib/internal/streams/stream-browser.js","rawRequest":"internal/streams/stream","loaders":[],"resource":"node_modules/readable-stream/lib/internal/streams/stream-browser.js","parser":{"type":"Parser","options":{},"sourceType":"auto","moduleType":"javascript/auto"},"generator":{"type":"JavascriptGenerator","moduleType":"javascript/auto","options":{}},"resolveOptions":{}}},"identifier":"node_modules/readable-stream/lib/internal/streams/stream-browser.js","assigned":{"factoryMeta":{},"issuer":"node_modules/readable-stream/lib/_stream_writable.js","useSourceMap":false,"lineToLine":false},"build":{"built":true,"buildTimestamp":1527104574698,"buildMeta":{"providedExports":true},"buildInfo":{"cacheable":true,"contextDependencies":[],"fileDependencies":["node_modules/read